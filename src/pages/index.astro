---
import "../styles/global.css";
import BaseLayout from "../layouts/BaseLayout.astro";
import Hero from "../components/Hero.astro";
import Foralle from "../components/Foralle.astro";
import Omulla from "../components/Omulla.astro";
import Ombehandlinger from "../components/Ombehandlinger.astro";
import Reviews from "../components/Reviews.astro";
import Tostole from "../components/Tostole.astro";
import Sidst from "../components/Sidst.astro";
const Title = "Holistisk Massage- & Zoneterapi";
---

<style>
  p {
    font-family: "Martel", serif;
    font-weight: 400;
    font-style: normal;
  }
</style>

<BaseLayout Title={Title}>
  <Hero />
  <Foralle />
  <Omulla />
  <Ombehandlinger />
  <Reviews />
  <Tostole />
  <Sidst />
</BaseLayout>
