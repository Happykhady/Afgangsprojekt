---
import "../styles/global.css";
import BaseLayout from "../layouts/BaseLayout.astro";
import NavigationLys from "../components/NavigationLys.astro";
import BlogFrame from "../components/BlogFrame.astro";
const Title = "Blog";
---

<style>
  div {
    background-color: var(--secondary-background);
  }

  p {
    font-family: "Martel", serif;
    font-weight: 400;
    font-style: normal;
  }
</style>

<BaseLayout Title={Title}>
  <NavigationLys />
  <BlogFrame />
</BaseLayout>
