---
import "../styles/global.css";
import BaseLayout from "../layouts/BaseLayout.astro";
import Uddannelse from "../components/Uddannelse.astro";
import Ulla from "../components/Ulla.astro";
import NavigationDark from "../components/NavigationDark.astro";
const Title = "Om";
---

<style>
  div {
    background-color: var(--tertiary-background);
  }

  p {
    font-family: "Martel", serif;
    font-weight: 400;
    font-style: normal;
  }
</style>

<BaseLayout Title={Title}>
  <NavigationDark />
  <Ulla />
  <Uddannelse />
</BaseLayout>
